<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.leoni.telephoneinfos.MainActivity$PlaceholderFragment">

    <TextView
        android:id="@+id/section_label"
        android:text="Telephone Infos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true" />


    <TextView
        android:id="@+id/label1"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:textSize="@dimen/font_size"
        />

    <TextView
        android:id="@+id/label2"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:textSize="@dimen/font_size"
        android:layout_below="@id/label1" />

    <TextView
        android:id="@+id/label3"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:textSize="@dimen/font_size"
        android:layout_below="@id/label2"/>

    <TextView
        android:id="@+id/label4"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:textSize="@dimen/font_size"
        android:layout_below="@id/label3"/>

    <TextView
        android:id="@+id/label5"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:textSize="@dimen/font_size"
        android:layout_below="@id/label4"/>
    <TextView
        android:id="@+id/label6"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:textSize="@dimen/font_size"
        android:layout_below="@id/label5"/>

    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_below="@id/label6">

    </ListView>


    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:apiKey="AIzaSyBTGKtt06EHDCoCKE3dlJMX2VU9quALgDI"/>

</RelativeLayout>
